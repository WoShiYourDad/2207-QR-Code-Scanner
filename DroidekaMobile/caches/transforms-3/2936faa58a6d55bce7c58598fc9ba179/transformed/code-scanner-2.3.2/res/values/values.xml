<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CodeScannerView">
        <attr format="color" name="maskColor"/>
        <attr format="boolean" name="maskVisible"/>
        <attr format="color" name="frameColor"/>
        <attr format="boolean" name="frameVisible"/>
        <attr format="dimension" name="frameThickness"/>
        <attr format="dimension" name="frameCornersSize"/>
        <attr format="dimension" name="frameCornersRadius"/>
        <attr format="boolean" name="frameCornersCapRounded"/>
        <attr format="float" name="frameAspectRatioWidth"/>
        <attr format="float" name="frameAspectRatioHeight"/>
        <attr format="float" name="frameSize"/>
        <attr format="float" name="frameVerticalBias"/>
        <attr format="color" name="autoFocusButtonColor"/>
        <attr format="boolean" name="autoFocusButtonVisible"/>
        <attr format="dimension" name="autoFocusButtonPaddingHorizontal"/>
        <attr format="dimension" name="autoFocusButtonPaddingVertical"/>
        <attr format="reference" name="autoFocusButtonOnIcon"/>
        <attr format="reference" name="autoFocusButtonOffIcon"/>
        <attr format="enum" name="autoFocusButtonPosition">
            <enum name="topStart" value="0"/>
            <enum name="topEnd" value="1"/>
            <enum name="bottomStart" value="2"/>
            <enum name="bottomEnd" value="3"/>
        </attr>
        <attr format="color" name="flashButtonColor"/>
        <attr format="boolean" name="flashButtonVisible"/>
        <attr format="dimension" name="flashButtonPaddingHorizontal"/>
        <attr format="dimension" name="flashButtonPaddingVertical"/>
        <attr format="reference" name="flashButtonOnIcon"/>
        <attr format="reference" name="flashButtonOffIcon"/>
        <attr format="enum" name="flashButtonPosition">
            <enum name="topStart" value="0"/>
            <enum name="topEnd" value="1"/>
            <enum name="bottomStart" value="2"/>
            <enum name="bottomEnd" value="3"/>
        </attr>
    </declare-styleable>
</resources>